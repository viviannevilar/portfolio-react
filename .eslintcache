, () => setOpen(false));\n\n   return (\n     <div>\n         {/* <Nav /> */}\n\n      \n        <Router id=\"body\">\n          <Switch>\n              <Route exact path=\"/\">\n                <Home />\n              </Route>\n              <Route exact path=\"/projects/\">\n                <Projects />\n              </Route>\n              <Route path=\"/contact\">\n                <Home />\n              </Route>\n          </Switch>\n        </Router>\n        <div ref={node}>\n          <Burger open={open} setOpen={setOpen} />\n          <Menu open={open} setOpen={setOpen} />\n        </div>\n\n    </div>\n  );\n}\n\nexport default App;\n","/Users/viviannevilar/SheCodes/Portfolio/portfolio-react/src/Pages/Projects.jsx",["81"],"/Users/viviannevilar/SheCodes/Portfolio/portfolio-react/src/index.js",[],"/Users/viviannevilar/SheCodes/Portfolio/portfolio-react/src/Pages/Python.jsx",["82","83","84"],"/Users/viviannevilar/SheCodes/Portfolio/portfolio-react/src/Components/Nav.jsx",[],"/Users/viviannevilar/SheCodes/Portfolio/portfolio-react/src/Components/Logo.jsx",[],"/Users/viviannevilar/SheCodes/Portfolio/portfolio-react/src/Components/ColesWrapper.jsx",[],"/Users/viviannevilar/SheCodes/Portfolio/portfolio-react/src/Components/SvgPythonGraph.jsx",[],"/Users/viviannevilar/SheCodes/Portfolio/portfolio-react/src/Components/IconStack.jsx",[],"/Users/viviannevilar/SheCodes/Portfolio/portfolio-react/src/Components/ConsoleLog.jsx",[],"/Users/viviannevilar/SheCodes/Portfolio/portfolio-react/src/firebase/firebaseConfig.js",[],"/Users/viviannevilar/SheCodes/Portfolio/portfolio-react/src/Pages/Contact.jsx",[],"/Users/viviannevilar/SheCodes/Portfolio/portfolio-react/src/Components/ContactForm.jsx",["85","86","87"],"/Users/viviannevilar/SheCodes/Portfolio/portfolio-react/src/Pages/Home.jsx",["88"],"////////////////// imports ///////////////////\nimport React, { useState, useEffect } from \"react\"\nimport { useHistory, useLocation } from \"react-router-dom\";\n//-----> external 