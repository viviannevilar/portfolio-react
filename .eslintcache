/Pages/Contact'\n//import Nav from './Components/Nav'\nimport Burger from './Components/Burger';\nimport Menu from './Components/Menu';\nimport { useOnClickOutside } from './hooks';\n\nfunction App() {\n\n  const [open, setOpen] = useState(false);\n  const node = useRef(); \n  useOnClickOutside(node, () => setOpen(false));\n\n   return (\n     <div>\n         {/* <Nav /> */}\n\n      \n        <Router id=\"body\">\n          <Switch>\n              <Route exact path=\"/\">\n                <Home />\n              </Route>\n              <Route path=\"/projects\">\n                <Projects />\n              </Route>\n              <Route path=\"/contact\">\n                <Home />\n              </Route>\n              <Route path=\"/about\">\n                <About />\n              </Route>\n          </Switch>\n        </Router>\n        <div ref={node}>\n          <Burger open={open} setOpen={setOpen} />\n          <Menu open={open} setOpen={setOpen} />\n        </div>\n\n    </div>\n  );\n}\n\nexport default App;\n","/Users/viviannevilar/SheCodes/Portfolio/portfolio-react/src/Pages/Projects.jsx",["86","87","88"],"/Users/viviannevilar/SheCodes/Portfolio/portfolio-react/src/index.js",[],"/Users/viviannevilar/SheCodes/Portfolio/portfolio-react/src/Pages/Python.jsx",["89","90","91"],"/Users/viviannevilar/SheCodes/Portfolio/portfolio-react/src/Components/Nav.jsx",[],"/Users/viviannevilar/SheCodes/Portfolio/portfolio-react/src/Components/Logo.jsx",[],"/Users/viviannevilar/SheCodes/Portfolio/portfolio-react/src/Components/ColesWrapper.jsx",[],"/Users/viviannevilar/SheCodes/Portfolio/portfolio-react/src/Components/SvgPythonGraph.jsx",[],"/Users/viviannevilar/SheCodes/Portfolio/portfolio-react/src/Components/IconStack.jsx",[],"/Users/viviannevilar/SheCodes/Portfolio/portfolio-react/src/Components/ConsoleLog.jsx",[],"/Users/viviannevilar/SheCodes/Portfolio/portfolio-react/src/firebase/firebaseConfig.js",[],"/Users/viviannevilar/SheCodes/Portfolio/portfolio-react/src/Pages/Contact.jsx",[],"/Users/viviannevilar/SheCodes/Portfolio/portfolio-react/src/Components/ContactForm.jsx",["92","93","94"],"import React, { useState } from 'react'\nimport { Axios, db } from '../firebase/firebaseConfig'\nimport './ContactForm.scss'\n\nconst ContactForm = (props) => {\n\n  const { passContactActive } = props\n\n  const [formData, setFormData] = useState({})\n  const [contactName, setContactName] = useState()\n  const [messageSent, setMessageSent] = useState(false)\n\n  \n  const [errorMessage, setErrorMessage] = useState();\n  const [errorKey, setErrorKey] = useState();\n\n  const [confirmation, setConfirmation] = useState(`Thank you, ${contactName}, your message has been sent!`)\n\n  const updateInput = e => {\n    setFormData({\n      ...formData,\n      [e.target.name]: e.target.value,\n    })\n  }\n\n  const handleSubmit = event => {\n    event.preventDefault()\n\n\n    if (!formData.name) {\n      setErrorKey(\"name\");\n      setErrorMessage(\"This field is mandatory\");\n    } \n\n    sendEmail()\n    setContactName(formData.name)\n    setMessageSent(true)\n    setFormData({\n      name: '',\n      email: '',\n      message: '',\n    })\n    \n  }\n\n  const sendEmail = () => {\n    Axios.post(\n      'https://us-central1-portfolio-425e2.cloudfunctions.net/submit',\n      formData\n    )\n      .then(res => {\n        db.collection('emails').add({\n          name: formData.name,\n          email: formData.email,\n          message: formData.message,\n          time: new Date(),\n        })\n      })\n      .catch(error => {\n        console.log(error)\n        setConfirmation(\"There was an error. Please try again or contact me via viviannevilar at gmail dot com.\")\n\n      })\n  }\n\n  const handleClick = event => {\n    event.preventDefault()\n    setMessageSent(false)\n  }\n\n  function handleClickClose() {\n    passContactActive(false)\n    setMessageSent(false)\n  }\n\n  // function handleClickClose() {\n  //   passContactActive(false)\n  //   setMessageSent(false)\n  // }\n\n  console.log(\"messageSent \", messageSent)\n\n  return (\n    <div className=\"contact-overlay-container\">\n      <form onSubmit={handleSubmit} className={messageSent ? \"overlay-inactive\" : \"overlay-active\"}>\n        <input\n          type=\"text\"\n          name=\"name\"\n          placeholder=\"Name\"\n          onChange={updateInput}\n          value={formData.name || ''}\n        />\n        <input\n          type=\"email\"\n          name=\"email\"\n          placeholder=\"Email\"\n          onChange={updateInput}\n          value={formData.email || ''}\n        />\n        <textarea\n          type=\"text\"\n          name=\"message\"\n          placeholder=\"Message\"\n          onChange={updateInput}\n          value={formData.message || ''}\n        ></textarea>\n        \n        <div className=\"button-wrapper\">\n          <button type=\"submit\">Submit</button>\n          <button type=\"button\" href='#' onClick={handleClickClose}>Cancel</button>\n          \n          {/* <a href='#' onClick={handleClickClose}>Cancel</a> */}\n        </div>\n      </form>\n\n      <div className={`contact-overlay ${messageSent ? \"overlay-active\" : \"overlay-inactive\"}`}>\n          <p>Thank you, {contactName}, your message has been sent!</p>\n          <div className=\"button-wrapper\">\n            <button className=\"button-a\" href='#' onClick={handleClick}>Send another message</button>\n            <button className=\"button-a\" href='#' onClick={handleClickClose}>Close</button>\n          </div>\n      </div>\n    </div>\n  )\n}\n\nexport default ContactForm\n\n\n\n\n///// overlay\n\n// .overlay {\n//   display: none;\n// }\n\n// .overlay-container:hover .overlay {\n//   display: block;\n//   // transition: 2s;\n// }\n\n// // might delete this, just seeing if it helps with deployed version\n// .overlay-container:hover .overlay-top {\n//   display: block;\n//   transition: 2s;\n// }\n\n// .overlay-container:hover .bf-overlay {\n//   filter: contrast(calc(0.5)) brightness(1);\n//  //filter: contrast(calc(0.5/1.5)) brightness(1.5);\n// }\n\n// .python-overlay-container {\n//   position: relative;\n// }","/Users/viviannevilar/SheCodes/Portfolio/portfolio-react/src/Pages/Home.jsx",["95","96"],"////////////////// imports ///////////////////\nimport React, { useState, useEffect } from \"react\"\nimport { useHistory, useLocation } from \"react-router-dom\";\n//-----> external packages \nimport { Helmet } from 'react-helmet'\n//-----> components\nimport ContactForm from '../Components/ContactForm'\n//-----> styles\nimport './Home.scss'\n//-----> images\nimport LogoCrop from \"../images/MeeCropSharp.png\"\n//import echo from \"../Components/ConsoleLog\";\nimport Pdf from '../images/CV_VivianneVilar.pdf';\n\n////////////////// main function ///////////////////\nfunction Home() {\n  const history = useHistory()\n  const location = useLocation()\n\n  const [contactActive, setContactActive] = useState(false)\n\n\n\n\n  useEffect(() => {\n    if (location.pathname === \"/contact/\") {\n      setContactActive(true)\n    }\n    console.log(\"contact active? \", contactActive)\n\n  },[location])\n\n  function handleClick(name) {\n    if (name === \"contact\") {\n      setContactActive(true)\n    } else {\n      history.push(`/${name}/`);\n    }\n\n  }\n\n\n  // useEffect(() => {\n  //   console.log(contactActive)\n  // }, [contactActive]);\n\n\n\n  console.log(\"-----------contactActive : \", contactActive)\n\n\n  return (\n    <div id=\"home\" className=\"page-wrapper\">\n        <Helmet>\n          <title>Vivianne Vilar - Home </title>\n          <meta name=\"viewport\" content=\"width=device-width, initial-scale=1, shrink-to-fit=no\" />\n        </Helmet>\n\n      <div id=\"image\">\n        <img id=\"main-img\" src={LogoCrop} alt=\"Icon-Large\"></img>\n      </div>\n    \n      <div id=\"my-info\" className={`${contactActive ? \"inactive\" : \"active\"}`}>\n        <h1>\n          Vivianne Vilar\n        </h1>\n        <p>\n          Economist, accidental data scientist, wannabe web developer.\n        </p>\n        <div class=\"buttons\">\n          <button onClick={() => handleClick(\"projects\")}>Projects</button>\n          <a className=\"button\" href = {Pdf} target = \"_blank\">CV</a>\n          {/* <button onClick=\"document.location='about'\">CV</button> */}\n          <button onClick={() => handleClick(\"contact\")}>Contact</button>\n\n        </div>\n      </div>\n\n      <div id=\"contact-form\" className={`${contactActive ? \"active\" : \"inactive\"}`}>\n        <ContactForm passContactActive={setContactActive} />\n      </div>\n\n    \n    </div>\n  )\n}\nexport default Home","/Users/viviannevilar/SheCodes/Portfolio/portfolio-react/src/Components/Burger.jsx",[],"/Users/viviannevilar/SheCodes/Portfolio/portfolio-react/src/Components/Menu.jsx",[],"/Users/viviannevilar/SheCodes/Portfolio/portfolio-react/src/hooks.js",[],"/Users/viviannevilar/SheCodes/Portfolio/portfolio-react/src/Pages/ProjectPages/ProjectPage.jsx",[],"/Users/viviannevilar/SheCodes/Portfolio/portfolio-react/src/Pages/About.jsx",[],{"ruleId":"97","severity":1,"message":"98","line":13,"column":8,"nodeType":"99","messageId":"100","endLine":13,"endColumn":15},{"ruleId":"97","severity":1,"message":"101","line":85,"column":9,"nodeType":"99","messageId":"100","endLine":85,"endColumn":17},{"ruleId":"97","severity":1,"message":"102","line":89,"column":9,"nodeType":"99","messageId":"100","endLine":89,"endColumn":17},{"ruleId":"103","severity":1,"message":"104","line":290,"column":21,"nodeType":"105","endLine":290,"endColumn":52},{"ruleId":"97","severity":1,"message":"106","line":2,"column":8,"nodeType":"99","messageId":"100","endLine":2,"endColumn":11},{"ruleId":"97","severity":1,"message":"107","line":3,"column":8,"nodeType":"99","messageId":"100","endLine":3,"endColumn":12},{"ruleId":"97","severity":1,"message":"108","line":4,"column":8,"nodeType":"99","messageId":"100","endLin